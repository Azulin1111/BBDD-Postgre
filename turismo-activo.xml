    <codigo>7</codigo>
    <idrecurso>4563</idrecurso>
    <nombre>BALKYSUB ESCUELA DE BUCEO </nombre>
    <urlportal>https://www.murciaturistica.es/es/empresa_turismo_activo/balkysub-escuela-de-buceo--4563/</urlportal>
    <tipo>turismo activo</tipo>
